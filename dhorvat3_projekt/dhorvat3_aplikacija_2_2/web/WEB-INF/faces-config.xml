<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>hr</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        
        <resource-bundle>
            <base-name>org.foi.nwtis.dhorvat3.i18n</base-name>
            <var>m</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{lokalizacija.odaberiJezik}</from-action>
            <from-outcome>PromjenaJezika</from-outcome>
            <to-view-id>\index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.prijava}</from-action>
            <from-outcome>PrijavaKorisnika</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.registracija}</from-action>
            <from-outcome>RegistracijaKorisnika</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.pocetna}</from-action>
            <from-outcome>Pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>Odjava</from-outcome>
            <to-view-id>\index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>NijePrijavljen</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.korisnici}</from-action>
            <from-outcome>PregledKorisnika</from-outcome>
            <to-view-id>\korisnici.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/prijava.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{lokalizacija.pocetna}</from-action>
            <from-outcome>Pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.registracija}</from-action>
            <from-outcome>RegistracijaKorisnika</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.prijava}</from-action>
            <from-outcome>PrijavaKorisnika</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{prijava.prijava()}</from-action>
            <from-outcome>PogresnoKorisnicko</from-outcome>
            <to-view-id>\prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{prijava.prijava()}</from-action>
            <from-outcome>PogresanPass</from-outcome>
            <to-view-id>\prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{prijava.prijava()}</from-action>
            <from-outcome>IspravanLogin</from-outcome>
            <to-view-id>\index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>Odjava</from-outcome>
            <to-view-id>\index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>NijePrijavljen</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.korisnici}</from-action>
            <from-outcome>PregledKorisnika</from-outcome>
            <to-view-id>\korisnici.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registracija.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{lokalizacija.pocetna}</from-action>
            <from-outcome>Pocetna</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.prijava}</from-action>
            <from-outcome>PrijavaKorisnika</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.registracija}</from-action>
            <from-outcome>RegistracijaKorisnika</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>Odjava</from-outcome>
            <to-view-id>\index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.odjava()}</from-action>
            <from-outcome>NijePrijavljen</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{registracija.registracija()}</from-action>
            <from-outcome>Uspjela</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{registracija.registracija()}</from-action>
            <from-outcome>Neuspjela</from-outcome>
            <to-view-id>/registracija.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{lokalizacija.korisnici}</from-action>
            <from-outcome>PregledKorisnika</from-outcome>
            <to-view-id>\korisnici.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
